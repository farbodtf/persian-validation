    "description": "پکیج اعتبارسنجی فارسی برای لاراول",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Farbod Taherifard",
            "email": "farbod.taherifard@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0",
        "ftf/persian-validation": "*"
    },
     "require-dev": {
